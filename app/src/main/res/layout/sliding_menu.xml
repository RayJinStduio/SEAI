        android:layout_width="wrap_content"
        android:layout_height="fill_parent"
        android:orientation="horizontal" >

        <include layout="@layout/menu_bar" />

        <include layout="@layout/main_view" />
    </LinearLayout>


</com.rayjin.seai.SlidingMenu>